<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="Hello world" Height="200" Width="300">
    <StackPanel>
        <TextBlock>
            Click this: 
            <Hyperlink x:Name="GoogleHyperlink" RequestNavigate="GoogleClick" NavigateUri="http://www.google.com">Google</Hyperlink>
        </TextBlock>
        <Label>
            <StackPanel Orientation="Horizontal">
                <Image Source="https://freetravelphotos-data.s3.amazonaws.com/uploads/photos/1622043633053.webp" Width="40"/>
                <AccessText Text="Message" FontSize="8"></AccessText>
            </StackPanel>
        </Label>
    </StackPanel>
</Window>
